		"bureaucrat": {
			"pageproperties-caneditproperties": true,
			"pageproperties-canmanageproperties": true
		},
		"pageproperties-admin": {
			"pageproperties-canmanageproperties": true,
			"pageproperties-caneditproperties": true
		},
		"pageproperties-editor": {
			"pageproperties-canmanageproperties": false,
			"pageproperties-caneditproperties": true
		}
	},
	"AddGroups": {
		"pageproperties-admin": [
			"pageproperties-admin",
			"pageproperties-editor"
		]
	},
	"RemoveGroups": {
		"pageproperties-admin": [
			"pageproperties-admin",
			"pageproperties-editor"
		]
	},
	"AvailableRights": [
		"pageproperties-caneditproperties",
		"pageproperties-canmanageproperties"
	],
